{
  "compilerOptions": {
    "module": "NodeNext",
    "lib": ["ESNext", "deno.ns"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "resolveJsonModule": true
  }
}
